import numpy as np
import pandas as pd
import tensorflow as tf
import matplotlib.pyplot as plt



data1=pd.read_csv("/content/train.csv")
data2=pd.read_csv("/content/test.csv")

data1.head()

women = data1.loc[data1.Sex == 'female']["Survived"]
rate_women = sum(women)/len(women)
rate_women

men = data1.loc[data1.Sex == 'male']["Survived"]
rate_men = sum(men)/len(men)
rate_men

from sklearn.ensemble import RandomForestClassifier

y = data1["Survived"]

features = ["Pclass", "Sex", "SibSp", "Parch"]
X = pd.get_dummies(data1[features])
X_test = pd.get_dummies(data2[features])

model = RandomForestClassifier(n_estimators=100, max_depth=5, random_state=1)
model.fit(X, y)
predictions = model.predict(X_test)

output = pd.DataFrame({'PassengerId': data2.PassengerId, 'Survived': predictions})
output.to_csv('submission.csv', index=False)
print("Your submission was successfully saved!")
